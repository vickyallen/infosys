#PF-Assgn-15

def find_product(num1,num2,num3):
    product=0
    if (num1==7 or num2==7 or num3==7):
        if(num1==7):
            product=num2*num3
            return product
        elif(num2==7):
            product=num3
            return product
        elif(num3==7):
            product=-1
            return product
    else:
        product =num1*num2*num3
        return product
#Provide different values for num1, num2, num3 and test your program
product=find_product(4,2,3)
print(product)
